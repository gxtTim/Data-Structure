class Solution {
    // Time: O(n), Space: O(1)
    public int majorityElement(int[] nums) {
        int count = 0;
        Integer res = null;
        
        for (int num : nums) {
            if (count == 0) {
                res = num;
            }
            if (num == res) {
                count++;
            } else {
                count--;
            }
        }
        return res;
    }
}
