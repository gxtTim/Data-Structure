class Solution {
    public boolean isValid(String s) {
        if (s == "") return true;
        Deque<Character> stack = new LinkedList<Character>();
        for (int i = 0; i < s.length(); i++) {
            if (s.charAt(i) == '(' || s.charAt(i) == '{' || 
                s.charAt(i) == '[') {
                stack.push(s.charAt(i));
            }
            else {
                if (stack.size() != 0) {
                  if (s.charAt(i) == ')') {
                        if (stack.peek() == '(') {
                            stack.pop();
                        }
                        else return false;
                    }
                    else if (s.charAt(i) == '}') {
                        if (stack.peek() == '{') {
                            stack.pop();
                        }
                        else return false;
                    }
                    else if (s.charAt(i) == ']') {
                        if (stack.peek() == '[') {
                            stack.pop();
                        }
                        else return false;
                    }
                    else {
                        return false;
                    }                
                }
                else return false;
            }
        }
        if (stack.size() == 0) return true;
        else return false;
    }
}
